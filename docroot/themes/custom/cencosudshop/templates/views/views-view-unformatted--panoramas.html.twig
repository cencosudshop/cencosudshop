{% if title %}
  <h3>{{ title }}</h3>
{% endif %}
<ol>
    {% for key, value in page  %}
        <li>{{ key }}</li>
    {% endfor %}
</ol>
<div class="row pm-3">
    <div class="col-md-8">
        <div class="img-container">
            <img src="/themes/custom/ccenter/images/sky_costanera.jpg">                
        </div>
    </div>
    <div class="col-md-4">
        <h4><strong>SKY COSTANERA</strong></h4>
        <p>Somos el mirador mas alto de Latinoamerica</p>      
        <p>
            <i class="far fa-clock"></i>
            Lunes a Domingo - 10:00 a 22:00 Horas</p>
        <p>
            <i class="fas fa-globe-americas"></i>
            <a href="http://www.skycostanera.cl"> skycostanera.cl</a>
        </p>
    </div>
</div>
{% if rows %}
    <div class="row">
    {% for row in rows %}
        {{ row.content }}
    {% endfor %}
    </div>
{% endif %}
